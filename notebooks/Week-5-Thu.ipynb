    "T_true = 40\n",
    "err0_y = 0.5\n",
    "\n",
    "t = 100 * np.random.random(N)\n",
    "y_true = GaussAndBkgd(t, b0_true, A_true, sigma_true, T_true)\n",
    "err_y = np.random.uniform(0, 2*err0_y) + 0.5*np.sqrt(y_true/10) \n",
    "y_obs = np.random.normal(y_true, err_y)\n",
    "\n",
    "labels = ['$b_0$', '$A$', '$T$', r'$\\sigma$']\n",
