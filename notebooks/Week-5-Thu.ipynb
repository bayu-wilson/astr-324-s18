    "#----------------------------------------------------------------------\n",
    "# Set up toy dataset\n",
    "def GaussAndBkgd(t, b0, A, sigW, T):\n",
    "    \"\"\"Gaussian profile + flat background model\"\"\"\n",
    "    y = np.empty(t.shape)\n",
    "    y.fill(b0)\n",
    "    y += A * np.exp(-(t - T)**2/2/sigW**2)\n",
    "    return y\n",
    "\n",
    "np.random.seed(42)\n",
    "N = 101\n",
    "b0_true = 10\n",
    "A_true = 3\n",
